{
  "name": "tampan-lint",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/tampan-lint/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/tampan-lint",
        "main": "packages/tampan-lint/src/index.js",
        "tsConfig": "packages/tampan-lint/tsconfig.lib.json",
        "assets": ["packages/tampan-lint/*.md"]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs tampan-lint {args.ver} {args.tag}"
      },
      "dependsOn": ["build"]
    },
    "release": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "npx semantic-release-plus --extends ./packages/tampan-lint/release.config.js",
        "parallel": false
      }
    }
  },
  "tags": []
}
