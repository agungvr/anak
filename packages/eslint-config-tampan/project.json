{
  "name": "eslint-config-tampan",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/eslint-config-tampan/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/eslint-config-tampan",
        "main": "packages/eslint-config-tampan/src/index.ts",
        "tsConfig": "packages/eslint-config-tampan/tsconfig.lib.json",
        "assets": ["packages/eslint-config-tampan/*.md"]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs eslint-config-tampan {args.ver} {args.tag}"
      },
      "dependsOn": ["build"]
    },
    "release": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "npx semantic-release-plus --extends ./packages/eslint-config-tampan/release.config.js",
        "parallel": false
      }
    }
  },
  "tags": []
}
